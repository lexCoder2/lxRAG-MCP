{
  "name": "@stratsolver/graph-server",
  "version": "0.0.1",
  "description": "MCP server for code graph analysis, test intelligence, and progress tracking",
  "author": "stratSolver Team",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/server.js",
    "start:http": "node scripts/start-http-supervisor.mjs",
    "start:http:raw": "MCP_TRANSPORT=http MCP_PORT=9000 node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "benchmark:check-regression": "python3 scripts/check_benchmark_regression.py"
  },
  "keywords": [
    "mcp",
    "code-graph",
    "memgraph",
    "test-intelligence",
    "progress-tracking"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "chokidar": "^5.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "neo4j-driver": "^5.14.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18"
  }
}
